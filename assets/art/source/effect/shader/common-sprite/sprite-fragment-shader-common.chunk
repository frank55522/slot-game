  precision highp float;

  #pragma builtin(local)
  layout(set = 2, binding = 10) uniform sampler2D cc_spriteTexture;

  in vec4 v_color;
  in vec2 v_uv;

  vec4 addSmooth () {
    vec4 o = texture(cc_spriteTexture, v_uv)*v_color;
    o.rgb *= o.a;
    return o;
  }

  vec4 premultiplied () {
    vec4 o = texture(cc_spriteTexture, v_uv)*v_color*v_color.a;
    return o;
}