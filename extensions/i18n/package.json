{
    "package_version": 2,
    "version": "1.1.19",
    "name": "i18n",
    "description": "i18n:i18n.description",
    "main": "./dist/main.js",
    "devDependencies": {
        "@types/node": "^16.9.1",
        "@types/vue": "^2.0.0",
        "typescript": "^4.3.4",
	"@types/fs-extra": "^5.0.4"
    },
    "panels": {
        "default": {
            "title": "i18n:i18n.title",
            "main": "./dist/panels/default.js"
        }
    },
    "author": "jumbo",
    "editor": ">=3.1.0",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    },
    "contributions": {
        "messages": {
            "open-panel": {
                "methods": [
                    "openPanel"
                ]
            }
        },
        "menu": [
            {
                "path": "i18n:menu.extension",
                "label": "i18n:i18n.menu",
                "message": "open-panel"
            }
        ],
        "scene": {
            "script": "./dist/scene.js"
        },
        "asset-db": {
            "mount": {
                "path": "./assets",
                "readonly": true
            }
        },
        "profile": {
            "project": {
                "first": {
                    "type": "boolean",
                    "default": false
                }
            }
        },
        "builder": "./dist/builder.js"
    },
    "dependencies": {
        "vue": "2.6.14"
    }
}
