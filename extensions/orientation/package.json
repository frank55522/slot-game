{
    "package_version": 2,
    "version": "1.0.1",
    "name": "orientation",
    "description": "i18n:orientation.description",
    "main": "./dist/main.js",
    "devDependencies": {
        "@types/node": "^16.9.1",
        "@types/vue": "^2.0.0",
        "typescript": "^4.3.4"
    },
    "panels": {
        "default": {
            "title": "i18n:orientation.name",
            "main": "./dist/panels/default.js"
        }
    },
    "author": "Anonymous",
    "editor": ">=3.1.0",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    },
    "contributions": {
        "messages": {
            "open-default-panel": {
                "methods": [
                    "openDefaultPanel"
                ]
            }
        },
        "menu": [
            {
                "path": "i18n:menu.extension",
                "label": "i18n:orientation.control-panel",
                "message": "open-default-panel"
            }
        ],
        "scene": {
            "script": "./dist/scene.js"
        },
        "asset-db": {
            "mount": {
                "path": "./assets",
                "readonly": true
            }
        },
        "profile": {
            "project": {
                "first": {
                    "type": "boolean",
                    "default": false
                }
            }
        }
    },
    "dependencies": {
        "vue": "^2.6.14"
    }
}
